<?xml version='1.0' encoding='UTF-8'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5">
  <display-name>TestLocalEjbServiceWebApp</display-name>
  <servlet>
    <servlet-name>TestLocalEjbServlethttp</servlet-name>
    <servlet-class>gov.va.vba.framework.services.ejb.local.TestLocalEjb</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestLocalEjbServlethttp</servlet-name>
    <url-pattern>/TestLocalEjb</url-pattern>
  </servlet-mapping>
<ejb-local-ref> 
  	<ejb-ref-name>ejb/TuxedoServiceV3Local</ejb-ref-name>
	<ejb-ref-type>Session</ejb-ref-type>
	<local>gov.va.vba.framework.services.TuxedoServiceLocalV3</local>
	<ejb-link>TuxedoServiceV3Local</ejb-link> 
</ejb-local-ref>
<ejb-local-ref> 
  	<ejb-ref-name>ejb/VeteranSessionEJBV2Local</ejb-ref-name>
	<ejb-ref-type>Session</ejb-ref-type>
	<local>gov.va.vba.framework.services.VeteranSessionLocalV2</local>
	<ejb-link>VeteranSessionEJBV2Local</ejb-link> 
</ejb-local-ref>
<ejb-local-ref> 
  	<ejb-ref-name>ejb/SecurityServiceV2Local</ejb-ref-name>
	<ejb-ref-type>Session</ejb-ref-type>
	<local>gov.va.vba.framework.services.CommonSecurityServiceLocalV2</local>
	<ejb-link>SecurityServiceV2Local</ejb-link> 
</ejb-local-ref>
</web-app>
